{
  "id": "birlikteyiz",
  "name": "Birlikteyiz",
  "version": "1.0.0",
  "description": "Emergency mesh network for disaster response. Real-time earthquake data, peer-to-peer communication, and offline-capable emergency coordination.",
  "author": "UNIBOS Team",
  "icon": "üåç",
  "license": "Proprietary",

  "capabilities": {
    "backend": true,
    "web": true,
    "mobile": false,
    "cli": true,
    "realtime": true
  },

  "dependencies": {
    "modules": [],
    "python": ["websockets", "geopy"],
    "system": []
  },

  "platforms": ["macos", "linux", "windows", "raspberry-pi"],

  "features": [
    "Real-time earthquake data from AFAD and EMSC",
    "Peer-to-peer mesh communication",
    "Offline message queue",
    "Emergency resource coordination",
    "Location-based alerts"
  ],

  "settings": {
    "earthquake_poll_interval": 300,
    "mesh_discovery_enabled": true,
    "offline_mode_enabled": true
  },

  "endpoints": {
    "api": "/api/v1/birlikteyiz/",
    "web": "/birlikteyiz/",
    "ws": "/ws/birlikteyiz/"
  },

  "category": "emergency",
  "tags": ["disaster", "earthquake", "mesh-network", "p2p", "offline"]
}
